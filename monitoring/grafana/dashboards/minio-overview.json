{
  "id": null,
  "uid": "minio-overview",
  "title": "MinIO Overview",
  "tags": [
    "auto-provisioned",
    "minio"
  ],
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Requests per Second",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(minio_http_requests_total[5m])) by (api)",
          "legendFormat": "{{api}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Request Errors",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(minio_http_requests_error_total[5m])) by (api)",
          "legendFormat": "{{api}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Bucket Size (bytes)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(minio_bucket_usage_size)",
          "legendFormat": "size"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Bucket Object Count",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(minio_bucket_usage_object_total)",
          "legendFormat": "objects"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    }
  ]
}

