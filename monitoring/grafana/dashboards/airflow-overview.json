{
  "id": null,
  "uid": "airflow-overview",
  "title": "Airflow Overview",
  "tags": [
    "auto-provisioned",
    "airflow"
  ],
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "DAG Runs (success/fail)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "increase(airflow_dagrun_success[5m])",
          "legendFormat": "success"
        },
        {
          "refId": "B",
          "expr": "increase(airflow_dagrun_failed[5m])",
          "legendFormat": "failed"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Task Failures",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "increase(airflow_task_instance_failed[5m])",
          "legendFormat": "failed"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Average DAG Duration (sec)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg_over_time(airflow_dagrun_duration[5m])",
          "legendFormat": "{{dag_id}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Scheduler Heartbeat",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(airflow_scheduler_heartbeat[5m])",
          "legendFormat": "heartbeat"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      }
    }
  ]
}

