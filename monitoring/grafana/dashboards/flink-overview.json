{
  "id": null,
  "uid": "flink-overview",
  "title": "Flink Overview",
  "tags": [
    "auto-provisioned",
    "flink"
  ],
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "editable": true,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Running Jobs",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(flink_jobmanager_job_numRunningJobs)",
          "legendFormat": "jobs"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Checkpoint Duration",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "avg(flink_jobmanager_job_lastCheckpointDuration)",
          "legendFormat": "{{job}}"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Task Throughput (records/s)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(flink_taskmanager_job_task_operator_numRecordsOut[1m]))",
          "legendFormat": "records out"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "TaskManager Memory Usage",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(flink_taskmanager_Status_JVM_Memory_Heap_Used)",
          "legendFormat": "heap used"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        }
      }
    }
  ]
}

